<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.shopping.model">
    
    <class name="Buy" table="buy">
    	
    	<id name="id" column="id">
    		<generator class="increment"></generator>
    	</id>
    	<property name="bno" column="bno"></property>
    	<property name="cno" column="cno"></property>
    	<property name="sno" column="sno"></property>
    	<property name="num" column="num"></property>
    	<property name="price" column="price"></property>
    	<property name="address" column="address"></property>
    	<property name="date" column="date"></property>
    	<property name="buystate" column="buystate"></property>
    	<property name="shipment" column="shipment"></property>
    	<property name="couriernumber" column="couriernumber"></property>
    </class>


</hibernate-mapping>